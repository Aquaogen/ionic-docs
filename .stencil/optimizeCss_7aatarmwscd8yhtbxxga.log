docs-document{grid-area:document;padding:2rem;position:relative;display:-ms-flexbox;display:flex;-ms-flex-direction:column;flex-direction:column}@media (max-width:700px){docs-document{padding:1.6rem}}docs-document>:first-child{margin-top:.5rem;color:var(--blackish)}docs-document main{-ms-flex:1;flex:1}.open-preview{-ms-flex-align:center;align-items:center;-ms-flex-item-align:start;align-self:start;background-color:#dfedff;border-radius:4px;display:-ms-inline-flexbox;display:inline-flex;font-size:13px;line-height:1;padding:8px}@media (min-width:1124px){.open-preview{display:none}}.open-preview>ion-icon{margin-right:.25em}.table-of-contents{padding-top:17px;display:block}.table-of-contents .toc-label{text-transform:uppercase;font-size:10px;font-weight:800;color:#abb2bf;letter-spacing:.1em}.table-of-contents .toc-list{margin-top:4px;padding-left:0;margin-bottom:20px}.table-of-contents .toc-item{margin-bottom:0;line-height:1em;list-style:none}.table-of-contents .toc-item a{padding:4px 0;font-size:13px;font-weight:500;line-height:16px;position:relative;display:inline-block;letter-spacing:-.02em;color:#3880ff;-webkit-transition:all .15s ease;transition:all .15s ease}.table-of-contents .toc-item a:hover{color:var(--blue);opacity:.7}main h1 a.linked,main h2 a.linked{color:inherit;font-weight:inherit;position:relative;outline:none}main h1 a.linked:before,main h2 a.linked:before{background-color:#16161d;opacity:0;height:16px;width:16px;content:"";-webkit-mask:url(/docs/assets/img/link.svg);mask:url(/docs/assets/img/link.svg);-webkit-mask-size:cover;mask-size:cover;position:absolute;left:-20px;top:5px;-webkit-transition:all .1s ease;transition:all .1s ease}main h1 a.linked:hover,main h2 a.linked:hover{color:var(--darker)}main h1 a.linked:hover:before,main h2 a.linked:hover:before{background-color:#16161d;opacity:1}main h3 a.linked{color:inherit;font-weight:inherit;position:relative;outline:none}main h3 a.linked:before{background-color:#16161d;opacity:0;height:12px;width:12px;c