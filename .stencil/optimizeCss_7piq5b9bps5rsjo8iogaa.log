docs-content card-link a{-webkit-transition:all .15s ease;transition:all .15s ease}card-link{display:block;line-height:normal}card-link a{display:block;min-height:100%;border-radius:6px;padding:40px 32px;-webkit-box-shadow:0 4px 8px rgba(0,30,84,.12),0 1px 3px rgba(0,30,84,.08);box-shadow:0 4px 8px rgba(0,30,84,.12),0 1px 3px rgba(0,30,84,.08)}card-link a:hover{-webkit-transform:translateY(-1px);transform:translateY(-1px);-webkit-box-shadow:0 8px 16px rgba(0,30,84,.1),0 3px 6px rgba(0,30,84,.08);box-shadow:0 8px 16px rgba(0,30,84,.1),0 3px 6px rgba(0,30,84,.08)}card-link stencil-route-link{color:inherit;display:block;height:100%}card-link.has-icon a{padding-left:106px;position:relative}card-link.has-icon a:after,card-link.has-icon a:before{background:no-repeat 0 0;position:absolute;left:0;content:""}card-link.has-icon a:after{-webkit-transition:opacity .2s ease;transition:opacity .2s ease;opacity:0}card-link.has-icon a:hover:after{opacity:1}card-link a,card-link stencil-route-link{color:inherit}card-link a:hover,card-link stencil-route-link:hover{text-decoration:none;color:inherit}card-link .card-link__title{color:#151c29;display:block;font-size:20px;font-weight:600;letter-spacing:normal;margin-bottom:16px}card-link .card-link__body{color:#768090;font-size:14px;font-weight:400;letter-spacing:-.016em;line-height:1.8em}