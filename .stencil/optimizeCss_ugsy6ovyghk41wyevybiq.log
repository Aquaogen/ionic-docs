tab-group{display:block;margin-bottom:2em;margin-top:1em}tab-group header{border-bottom:1px solid rgba(0,32,88,.1);margin-bottom:1rem;white-space:nowrap}tab-group .tab-link{color:#a5acbb;cursor:pointer;display:inline-block;font-size:10px;font-weight:600;letter-spacing:.04em;padding:12px 16px;position:relative;text-transform:uppercase;-webkit-transition:all .1s ease;transition:all .1s ease}tab-group .tab-link:after{background-color:transparent;content:"";display:block;height:1px;left:0;position:absolute;top:100%;width:100%}tab-group .tab-link.is-selected{color:var(--blue);font-weight:700}tab-group .tab-link.is-selected:after{background-color:var(--blue)}tab-group .tab-link:hover{color:var(--blue)}tab-group .tab{display:none}tab-group .tab.is-selected{display:block}