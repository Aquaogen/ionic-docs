color-gen-select-colors{display:block;padding-top:20px;margin-bottom:80px}.controls,.controls__sub{margin:0;padding:0}.controls li{list-style-type:none}.controls__item{margin:0;position:relative;border-bottom:1px solid var(--whiteish)}.controls__item>color-gen-variable-selector{cursor:pointer}.controls__item svg{-webkit-transition:-webkit-transform .1s;transition:-webkit-transform .1s;transition:transform .1s;transition:transform .1s,-webkit-transform .1s;position:absolute;top:24px;right:12px;pointer-events:none}.controls__item:first-child{border-top:1px solid var(--whiteish)}.controls__sub>li{height:0;opacity:0;margin:0;pointer-events:none;-webkit-transition:height .16s,opacity .16s;transition:height .16s,opacity .16s}.controls__item.is-open svg{-webkit-transform:rotate(180deg);transform:rotate(180deg)}.controls__item.is-open .controls__sub>li{height:54px;opacity:1;pointer-events:all}